<?xml version="1.0" encoding="UTF-8"?>
<hazelcast-client xsi:schemaLocation="http://www.hazelcast.com/schema/client-config hazelcast-client-config-3.10.xsd"
                  xmlns="http://www.hazelcast.com/schema/client-config"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <group>
        <name>jet</name>
        <password>jet-pass</password>
    </group>
    <properties>
        <property name="hazelcast.client.invocation.timeout.seconds">300</property>
    </properties>
    <network>
        <cluster-members>
            <address>192.168.99.1</address>
        </cluster-members>
        <connection-attempt-limit>999999</connection-attempt-limit>
    </network>
    <user-code-deployment enabled="true">
        <classNames>
            <className>tests.snapshot.QueueVerifier</className>
            <className>tests.eventjournal.EventJournalConsumer</className>
            <className>tests.eventjournal.EventJournalTradeProducer</className>
            <className>jet.com.hazelcast.tests.eventjournal.EventJournalTest</className>
        </classNames>
    </user-code-deployment>
</hazelcast-client>
